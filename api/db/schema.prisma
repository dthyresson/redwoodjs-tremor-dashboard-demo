datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Kpi {
  id        Int     @id @default(autoincrement())
  title     String
  metric    String
  progress  Decimal
  target    String
  delta     String
  deltaType String
}
